{
    "name": "@osynco/web",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "pnpm with-env next build",
        "clean": "git clean -xdf .cache .next .turbo node_modules",
        "dev": "pnpm with-env next dev --turbopack --port 6462",
        "tunnel": "devtunnel host osynco --allow-anonymous",
        "format": "prettier --check . --ignore-path ../../.gitignore",
        "lint": "eslint",
        "start": "pnpm with-env next start",
        "typecheck": "tsc --noEmit",
        "with-env": "dotenv -e ../../.env --"
    },
    "dependencies": {
        "@osynco/api": "workspace:*",
        "@osynco/auth": "workspace:*",
