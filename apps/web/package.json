        "@trpc/server": "catalog:",
        "@trpc/tanstack-react-query": "catalog:",
        "@upstash/qstash": "^2.8.1",
        "@upstash/workflow": "^0.2.13",
        "geist": "^1.3.1",
        "luxon": "^3.6.1",
        "nanoid": "^5.1.5",
        "next": "^15.2.3",
        "nuqs": "^2.4.3",
        "react": "catalog:react19",
